def saving(save_item1, save_item2):
    file=open("CodeFeast_data.txt","w")
    line=save_item1+"*"+save_item2
    file.write(line)
    file.close()

def retrieve():
    file=open("CodeFeast_data.txt","r")
    line=file.readline()
    line=line.split("*")
    print(line)
    tasks=line[0]
    deadlines=line[1]
    tasks=tasks[1:-1]
    tasks=tasks.split(",")
    deadlines=deadlines[1:-1]
    deadlines=deadlines.split(",")
